{
    "name": "kyvio/mailvio-api",
    "description": "Official Mailvio provided RESTFul API php library",
    "keywords": [
        "mailvio",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/KVSocial/mailvio-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mailvio Developers",
            "homepage": "https://app.mailvio.com/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
